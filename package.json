{
  "name": "start-page",
  "version": "1.0.0",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "deploy": "npm run stash:all && npm run build && docker-compose up -d && git stash pop",
    "stash:all": "git stash --include-untracked",
    "dev:server": "nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/src/index.ts",
    "build:server": "tsc"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@types/node": "^14.0.13",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.17.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "sirv-cli": "^1.0.1",
    "svelte": "^3.49.0",
    "tailwind-nord": "^1.1.0",
    "tailwindcss": "^1.4.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "license": "MIT",
  "author": {
    "name": "Shaiful Aiman bin Malik",
    "email": "ibnumalik@tbd.my"
  },
  "private": true,
  "dependencies": {
    "fastify": "^2.15.1"
  }
}
